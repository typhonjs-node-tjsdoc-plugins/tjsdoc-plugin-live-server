{
  "name": "tjsdoc-plugin-live-server",
  "version": "0.0.1",
  "description": "Provides a plugin that starts `live-server` providing refresh of the output doc target during TJSDoc execution.",
  "author": "typhonrt",
  "homepage": "https://tjsdoc.typhonjs.io/",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/typhonjs-node-tjsdoc-plugins/tjsdoc-plugin-live-server"
  },
  "scripts": {
    "build": "babel-node ./node_modules/typhonjs-npm-scripts-build-babel/scripts/build.js",
    "eslint": "eslint .",
    "dev-tjsdoc": "BABEL_ENV=tjsdoc-dev babel-node ../../typhonjs-node-tjsdoc/tjsdoc-babylon/src/TJSDocBabylonCLI.js -c .tjsdocrc-local",
    "prepublish": "babel-node ./node_modules/typhonjs-npm-scripts-publish/scripts/prepublish.js"
  },
  "dependencies": {
    "babel-runtime": "^6.0.0",
    "typhonjs-live-server": "latest"
  },
  "devDependencies": {
    "typhonjs-config-eslint": "^0.5.0",
    "typhonjs-npm-build-test": "^0.6.0"
  },
  "keywords": [
    "typhonjs",
    "tjsdoc",
    "plugin",
    "live",
    "server"
  ],
  "files": [
    "dist",
    "src",
    ".tjsdocrc",
    "AUTHORS.md"
  ],
  "main": "dist/TJSDocLiveServer.js",
  "bugs": {
    "url": "https://github.com/typhonjs-node-tjsdoc/tjsdoc/issues"
  }
}
